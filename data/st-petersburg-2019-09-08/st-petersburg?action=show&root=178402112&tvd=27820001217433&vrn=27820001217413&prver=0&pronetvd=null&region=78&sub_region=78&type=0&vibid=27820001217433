<script type="text/javascript">
	
		function open_close(group_id)
		{			
			folder = document.getElementById(group_id + "1");
			i = 1;
			while (folder != null) {
				if (folder.className == 'trFinrepHidden') {
					folder.className = 'trFinrep';
				} else {
					folder.className = 'trFinrepHidden';
				}
				i++;		
				folder = document.getElementById(group_id + "" + i);		
			}		
		}
	
</script>

